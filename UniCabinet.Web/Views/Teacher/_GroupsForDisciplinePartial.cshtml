@model List<UniCabinet.Core.Models.ViewModel.Group.GroupListVM>

<h3>Группы по дисциплине</h3>
<ul>
    @foreach (var group in Model)
    {
        <li>
            <a asp-controller="Teacher"
               asp-action="GroupStudentsProgress"
               asp-route-groupId="@group.Id"
               asp-route-disciplineId="@ViewBag.DisciplineId">
                @group.Name
            </a>
        </li>
    }
</ul>
